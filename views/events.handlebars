<div class="container">
	<div class="row">
		<div class="col-md-2"></div>
		<div class="col-md-8 panel panel-default">
			<h1>Upcoming Events</h1>
			<hr>
		
			
			{{!-- <div class="panel panel-default"> --}}
				<div class="panel-body">
			{{#each Events}}
			    <h2>{{this.name}}<small>&nbsp;&nbsp;{{this.date}}</small></h2>
				<h5><span>Time:</span> {{this.time}}   ||   <span>Venue:</span> {{this.venue}}</h5>
				<h5><span>Category:</span> {{this.category}}</h5>
			    <p><span>Details:</span> {{this.description}}</p>
			    <hr>
			{{/each}}
				</div>
			{{!-- </div> --}}
		</div>
		
		<div class="col-md-2"></div>
	</div>
</div>